    "db:create:test": "./node_modules/.bin/sequelize-cli db:create --env test",
    "db:drop:test": "./node_modules/.bin/sequelize-cli db:drop --env test",
    "db:migrate:test": "./node_modules/.bin/sequelize-cli db:migrate --env test",

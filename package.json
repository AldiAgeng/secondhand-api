{
  "name": "secondhand-api",
  "version": "1.0.0",
  "main": "app/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node bin/www",
    "develop": "nodemon -q bin/www",
    "test": "jest --coverage",
    "db:create": "./node_modules/.bin/sequelize-cli db:create",
    "db:drop": "./node_modules/.bin/sequelize-cli db:drop",
